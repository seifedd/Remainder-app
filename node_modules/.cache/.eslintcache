    </div>\n    </div>\n  );\n}\n\nfunction Today(person) {\n  let TodayDay = new Date().getDay();\n  let TodayMonth = new Date().getMonth();\n\n  let filter = person.filter((data) => {\n    let Day = new Date(data.birthday).getDay();\n    let month = new Date(data.birthday).getMonth();\n\n    return TodayDay === Day && TodayMonth === month;\n  });\n  return filter;\n}\n\nfunction Upcoming(person, toMonth = 7) {\n  let TodayDay = new Date().getDay();\n  let TodayMonth = new Date().getMonth();\n\n  let filter = person.filter((data) => {\n    let Day = new Date(data.birthday).getDay();\n    let Month = new Date(data.birthday).getMonth();\n    if (Day === TodayDay) return;\n    return TodayMonth <= Month && Month <= TodayMonth + toMonth;\n  });\n\n  return filter;\n}\n","/Users/seifmejri/Desktop/shiptCoding/Remainder-app/src/components/list.js",[],[],[],{"ruleId":"30","severity":1,"message":"31","line":73,"column":27,"nodeType":"32","messageId":"33","endLine":73,"endColumn":34},"array-callback-return","Array.prototype.filter() expects a return value from arrow function.","ReturnStatement","expectedReturnValue"]